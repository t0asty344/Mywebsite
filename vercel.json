{
  "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate",
        "Content-Encoding": "gzip"
      },
        "dest": "/index.html"
      }
    ]
  }